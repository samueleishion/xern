{
  "name": "xern",
  "version": "1.0.0",
  "description": "XERN React-Express-Node architecture without database",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm-run-all --serial setup:dirs build:prod",
    "setup:dirs": "mkdirp static/scrs static/stys",
    "build:dev": "npm-run-all serve \"gulp dev\"",
    "build:prod": "gulp prod",
    "lint:prettier": "./node_modules/prettier/bin/prettier.js --config ./.prettierrc --write \"./src/**/*.js\" --write \"./src/**/*.jsx\"",
    "lint:eslint": "./node_modules/eslint/bin/eslint.js --config ./eslintrc.json --fix --ext .js,.jsx ./src",
    "lint": "npm-run-all lint:*",
    "serve": "node index.js",
    "clean": "npm-run-all --parallel \"rimraf static/scrs\" \"rimraf static/stys\" \"rimraf node_modules\"",
    "validate": "npm ls",
    "run-node": "run-node",
    "cpr": "cpr",
    "mkdirp": "mkdirp",
    "rimraf": "rimraf",
    "npm-run-all": "npm-run-all"
  },
  "author": "samueleishion",
  "license": "ISC",
